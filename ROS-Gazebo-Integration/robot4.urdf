<?xml version="1.0"?>
<robot name="visual">

  <material name="blue"><color rgba="0 0 0.8 1"/></material>
  <material name="black"><color rgba="0 0 0 1"/></material>
  <material name="white"><color rgba="1 1 1 1"/></material>
  <material name="yellow"><color rgba="1 1 0 1"/></material>
  <material name="red"><color rgba="1 0 0 1"/></material>

  <link name="base_link">
    <visual>
      <geometry><cylinder length="0.4" radius="0.2"/></geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry><cylinder length="0.4" radius="0.2"/></geometry>
    </collision>
    <inertial>
      <mass value="5.0"/>
      <inertia ixx="0.1167" ixy="0" ixz="0" iyy="0.1167" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <link name="right_leg">
    <visual>
      <geometry><box size="0.5 0.1 0.2"/></geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.25"/>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry><box size="0.5 0.1 0.2"/></geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.25"/>
    </collision>
    <inertial>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.25"/>
      <mass value="1.0"/>
      <inertia ixx="0.0042" ixy="0" ixz="0" iyy="0.0242" iyz="0" izz="0.0217"/>
    </inertial>
  </link>

  <joint name="base_to_right_leg" type="fixed">
    <parent link="base_link"/><child link="right_leg"/>
    <origin xyz="0 -0.22 0.25"/>
  </joint>

  <link name="right_base">
    <visual>
      <geometry><box size="0.4 0.1 0.1"/></geometry>
      <material name="red"/>
    </visual>
    <collision>
      <geometry><box size="0.4 0.1 0.1"/></geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.00083" ixy="0" ixz="0" iyy="0.00708" iyz="0" izz="0.00708"/>
    </inertial>
  </link>

  <joint name="right_base_joint" type="fixed">
    <parent link="right_leg"/><child link="right_base"/>
    <origin xyz="0 0 -0.5"/>
  </joint>

  <link name="right_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.000227" ixy="0" ixz="0" iyy="0.000227" iyz="0" izz="0.0001225"/>
    </inertial>
  </link>

  <joint name="right_front_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="right_base"/><child link="right_front_wheel"/>
    <origin rpy="0 0 0" xyz="0.1333 0 -0.085"/>
  </joint>

  <link name="right_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.000227" ixy="0" ixz="0" iyy="0.000227" iyz="0" izz="0.0001225"/>
    </inertial>
  </link>

  <joint name="right_back_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="right_base"/><child link="right_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.1333 0 -0.085"/>
  </joint>

  <link name="left_leg">
    <visual>
      <geometry><box size="0.5 0.1 0.2"/></geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.25"/>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry><box size="0.5 0.1 0.2"/></geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.25"/>
    </collision>
    <inertial>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.25"/>
      <mass value="1.0"/>
      <inertia ixx="0.0042" ixy="0" ixz="0" iyy="0.0242" iyz="0" izz="0.0217"/>
    </inertial>
  </link>

  <joint name="base_to_left_leg" type="fixed">
    <parent link="base_link"/><child link="left_leg"/>
    <origin xyz="0 0.22 0.25"/>
  </joint>

  <link name="left_base">
    <visual>
      <geometry><box size="0.4 0.1 0.1"/></geometry>
      <material name="red"/>
    </visual>
    <collision>
      <geometry><box size="0.4 0.1 0.1"/></geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.00083" ixy="0" ixz="0" iyy="0.00708" iyz="0" izz="0.00708"/>
    </inertial>
  </link>

  <joint name="left_base_joint" type="fixed">
    <parent link="left_leg"/><child link="left_base"/>
    <origin xyz="0 0 -0.5"/>
  </joint>

  <link name="left_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.000227" ixy="0" ixz="0" iyy="0.000227" iyz="0" izz="0.0001225"/>
    </inertial>
  </link>

  <joint name="left_front_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="left_base"/><child link="left_front_wheel"/>
    <origin rpy="0 0 0" xyz="0.1333 0 -0.085"/>
  </joint>

  <link name="left_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry><cylinder length="0.1" radius="0.035"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.000227" ixy="0" ixz="0" iyy="0.000227" iyz="0" izz="0.0001225"/>
    </inertial>
  </link>

  <joint name="left_back_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="left_base"/><child link="left_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.1333 0 -0.085"/>
  </joint>

  <link name="head">
    <visual>
      <geometry><sphere radius="0.15"/></geometry>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry><sphere radius="0.15"/></geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.018" ixy="0" ixz="0" iyy="0.018" iyz="0" izz="0.018"/>
    </inertial>
  </link>

  <joint name="head_swivel" type="continuous">
    <parent link="base_link"/><child link="head"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.25"/>
  </joint>

  <link name="left_horn">
    <visual>
      <geometry><cylinder length="0.1" radius="0.02"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry><cylinder length="0.1" radius="0.02"/></geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.000093" ixy="0" ixz="0" iyy="0.000093" iyz="0" izz="0.00002"/>
    </inertial>
  </link>

  <joint name="head_to_left_horn" type="fixed">
    <parent link="head"/><child link="left_horn"/>
    <origin rpy="0 0.5 0" xyz="0 0.1 0.12"/>
  </joint>

  <link name="right_horn">
    <visual>
      <geometry><cylinder length="0.1" radius="0.02"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry><cylinder length="0.1" radius="0.02"/></geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.000093" ixy="0" ixz="0" iyy="0.000093" iyz="0" izz="0.00002"/>
    </inertial>
  </link>

  <joint name="head_to_right_horn" type="fixed">
    <parent link="head"/><child link="right_horn"/>
    <origin rpy="0 -0.5 0" xyz="0 -0.1 0.12"/>
  </joint>
  
  <gazebo>
   <plugin filename="gz-sim-diff-drive-system" name="gz::sim::systems::DiffDrive">
     <left_joint>left_front_wheel_joint</left_joint>
     <right_joint>right_front_wheel_joint</right_joint>
     <wheel_separation>0.44</wheel_separation>
     <wheel_radius>0.035</wheel_radius>
     <topic>cmd_vel</topic>
   </plugin>
  </gazebo>


</robot>