<?xml version="1.0" ?>
<sdf version="1.8">
    <world name="environment_only">
        <physics name="1ms" type="ignored">
            <max_step_size>0.001</max_step_size>
            <real_time_factor>1.0</real_time_factor>
        </physics>
        <plugin filename="gz-sim-physics-system" name="gz::sim::systems::Physics"></plugin>
        <plugin filename="gz-sim-user-commands-system" name="gz::sim::systems::UserCommands"></plugin>
        <plugin filename="gz-sim-scene-broadcaster-system" name="gz::sim::systems::SceneBroadcaster"></plugin>
        <plugin filename="gz-sim-sensors-system" name="gz::sim::systems::Sensors">
            <render_engine>ogre2</render_engine>
        </plugin>
        
        <plugin filename="gz-sim-contact-system" name="gz::sim::systems::Contact"></plugin>

        <gui fullscreen="0">
			<plugin filename="MinimalScene" name="3D View">
				<gz-gui>
					<title>3D View</title>
					<property type="bool" key="showTitleBar">false</property>
					<property type="string" key="state">docked</property>
				</gz-gui>
				<engine>ogre2</engine>
				<scene>scene</scene>
				<ambient_light>0.4 0.4 0.4</ambient_light>
				<background_color>0.8 0.8 0.8</background_color>
				<camera_pose>-6 0 6 0 0.5 0</camera_pose>
			</plugin>

			<plugin filename="GzSceneManager" name="Scene Manager"></plugin>
			<plugin filename="InteractiveViewControl" name="Interactive view control"></plugin>
			
          <plugin filename="VideoRecorder" name="VideoRecorder">
            <gz-gui>
              <property key="resizable" type="bool">false</property>
              <property key="x" type="double">300</property>
              <property key="y" type="double">50</property>
              <property key="width" type="double">50</property>
              <property key="height" type="double">50</property>
              <property key="state" type="string">floating</property>
              <property key="showTitleBar" type="bool">false</property>
              <property key="cardBackground" type="string">#777777</property>
            </gz-gui>

            <record_video>
              <use_sim_time>true</use_sim_time>
              <lockstep>false</lockstep>
              <bitrate>4000000</bitrate>
            </record_video>
          </plugin>

			<plugin filename="WorldControl" name="World control">
				<gz-gui>
					<title>World control</title>
					<property type="bool" key="showTitleBar">false</property>
					<property type="string" key="state">docked</property>
				</gz-gui>
				<play_pause>true</play_pause>
				<step>true</step>
				<start_paused>false</start_paused>
			</plugin>
		</gui>

        <light type="directional" name="sun">
            <cast_shadows>true</cast_shadows>
            <pose>0 0 10 0 0 0</pose>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
            <attenuation>
                <range>1000</range>
                <constant>0.9</constant>
                <linear>0.01</linear>
                <quadratic>0.001</quadratic>
            </attenuation>
            <direction>-0.5 0.1 -0.9</direction>
        </light>

        <model name="ground_plane">
            <static>true</static>
            <link name="link">
                <collision name="collision">
                    <geometry><plane><normal>0 0 1</normal></plane></geometry>
                </collision>
                <visual name="visual">
                    <geometry><plane><normal>0 0 1</normal><size>100 100</size></plane></geometry>
                    <material>
                        <ambient>0.8 0.8 0.8 1</ambient>
                        <diffuse>0.8 0.8 0.8 1</diffuse>
                        <specular>0.8 0.8 0.8 1</specular>
                    </material>
                </visual>
            </link>
        </model>

        <model name='arena_walls'>
            <static>true</static>
            <pose>0 0 0 0 0 0</pose>
            <link name='walls_link'>
                <collision name='collision_n'>
                    <pose>0 10 2.0 0 0 0</pose>
                    <geometry><box><size>20.5 0.5 4.0</size></box></geometry>
                </collision>
                <visual name='visual_n'>
                    <pose>0 10 2.0 0 0 0</pose>
                    <geometry><box><size>20.5 0.5 4.0</size></box></geometry>
                    <material><ambient>1 0 0 1</ambient><diffuse>1 0 0 1</diffuse></material>
                </visual>
                <collision name='collision_s'>
                    <pose>0 -10 2.0 0 0 0</pose>
                    <geometry><box><size>20.5 0.5 4.0</size></box></geometry>
                </collision>
                <visual name='visual_s'>
                    <pose>0 -10 2.0 0 0 0</pose>
                    <geometry><box><size>20.5 0.5 4.0</size></box></geometry>
                    <material><ambient>1 0 0 1</ambient><diffuse>1 0 0 1</diffuse></material>
                </visual>
                <collision name='collision_e'>
                    <pose>10 0 2.0 0 0 0</pose>
                    <geometry><box><size>0.5 20.5 4.0</size></box></geometry>
                </collision>
                <visual name='visual_e'>
                    <pose>10 0 2.0 0 0 0</pose>
                    <geometry><box><size>0.5 20.5 4.0</size></box></geometry>
                    <material><ambient>1 0 0 1</ambient><diffuse>1 0 0 1</diffuse></material>
                </visual>
                <collision name='collision_w'>
                    <pose>-10 0 2.0 0 0 0</pose>
                    <geometry><box><size>0.5 20.5 4.0</size></box></geometry>
                </collision>
                <visual name='visual_w'>
                    <pose>-10 0 2.0 0 0 0</pose>
                    <geometry><box><size>0.5 20.5 4.0</size></box></geometry>
                    <material><ambient>1 0 0 1</ambient><diffuse>1 0 0 1</diffuse></material>
                </visual>
            </link>
        </model>

        <model name='massive_obstacles'>
            <static>true</static>
            <link name='link'>
                <collision name='cube_1_vertical_col'>
                    <pose>5 1.0 1.0 0 0 0</pose> <geometry><box><size>2 10 2</size></box></geometry> </collision>
                <visual name='cube_1_vertical_vis'>
                    <pose>5 1.0 1.0 0 0 0</pose>
                    <geometry><box><size>2 10 2</size></box></geometry>
                    <material><ambient>1 0.5 0 1</ambient><diffuse>1 0.5 0 1</diffuse></material>
                </visual>
                <collision name='cube_1_horizontal_col'>
                    <pose>2 5 1.0 0 0 0</pose> <geometry><box><size>4 2 2</size></box></geometry> </collision>
                <visual name='cube_1_horizontal_vis'>
                    <pose>2 5 1.0 0 0 0</pose>
                    <geometry><box><size>4 2 2</size></box></geometry>
                    <material><ambient>1 0.5 0 1</ambient><diffuse>1 0.5 0 1</diffuse></material>
                </visual>

                <collision name='cube_2_col'>
                    <pose>-5 -5 1.0 0 0 0</pose>
                    <geometry><box><size>2 2 2</size></box></geometry>
                </collision>
                <visual name='cube_2_vis'>
                    <pose>-5 -5 1.0 0 0 0</pose>
                    <geometry><box><size>2 2 2</size></box></geometry>
                    <material><ambient>0 1 1 1</ambient><diffuse>0 1 1 1</diffuse></material>
                </visual>

                <collision name='cube_4_col'>
                    <pose>-6 5 1.5 0 0 0</pose> <geometry><box><size>8 3 3</size></box></geometry> </collision>
                <visual name='cube_4_vis'>
                    <pose>-6 5 1.5 0 0 0</pose>
                    <geometry><box><size>8 3 3</size></box></geometry>
                    <material><ambient>0.5 0 0.5 1</ambient><diffuse>0.5 0 0.5 1</diffuse></material>
                </visual>
            </link>
        </model>
    </world>
</sdf>